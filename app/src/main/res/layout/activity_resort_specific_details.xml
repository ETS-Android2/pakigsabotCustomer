<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cebu_wetland_details"
    tools:context=".Resorts.ResortSpecificDetails">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.753"
        app:srcCompat="@drawable/white_rect2" />

    <ImageView
        android:id="@+id/closeIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageView16"
        app:layout_constraintVertical_bias="0.043"
        app:srcCompat="@drawable/ic_close" />

    <TextView
        android:id="@+id/cebuWetlandResortTxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:fontFamily="@font/comforta"
        android:text="Cebu Wetland Resort"
        android:textColor="@color/darkgreen"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageView16"
        app:layout_constraintVertical_bias="0.044" />

    <TextView
        android:id="@+id/policiesTxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:fontFamily="@font/poppins_light"
        android:text="• Where to Sleep?"
        android:textColor="@color/gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.335"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/cebuWetlandResortTxt"
        app:layout_constraintVertical_bias="0.075" />

    <ImageView
        android:id="@+id/bedIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/policiesTxt"
        app:layout_constraintVertical_bias="0.075"
        app:srcCompat="@drawable/ic_bed" />

    <TextView
        android:id="@+id/bedroomTxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:fontFamily="@font/poppins_light"
        android:text="Bedroom - 1 King Bed"
        android:textColor="@color/gray"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toEndOf="@+id/bedIcon"
        app:layout_constraintTop_toTopOf="@+id/policiesTxt"
        app:layout_constraintVertical_bias="0.101" />

    <ImageView
        android:id="@+id/bed2Icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/bedIcon"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_sofabed" />

    <TextView
        android:id="@+id/sofabedTxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:fontFamily="@font/poppins_light"
        android:text="Common Space - 1 Sofa Bed"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toEndOf="@+id/bed2Icon"
        app:layout_constraintTop_toTopOf="@+id/bedroomTxt"
        app:layout_constraintVertical_bias="0.118" />

    <TextView
        android:id="@+id/roomATxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:fontFamily="@font/poppins_light"
        android:text="• Room A offers:"
        android:textColor="@color/gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.335"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/bed2Icon"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/beachIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.228"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/roomATxt"
        app:layout_constraintVertical_bias="0.097"
        app:srcCompat="@drawable/ic_beach" />

    <TextView
        android:id="@+id/beachTxt"
        android:layout_width="204dp"
        android:layout_height="49dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/poppins_light"
        android:text="Beach access - Beachfront"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toEndOf="@+id/beachIcon"
        app:layout_constraintTop_toTopOf="@+id/roomATxt"
        app:layout_constraintVertical_bias="0.088" />

    <ImageView
        android:id="@+id/wifiIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.228"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/beachIcon"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_wifi" />

    <TextView
        android:id="@+id/wifiTxt"
        android:layout_width="45dp"
        android:layout_height="22dp"
        android:fontFamily="@font/poppins_light"
        android:text="Wifi"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toEndOf="@+id/wifiIcon"
        app:layout_constraintTop_toTopOf="@+id/beachTxt"
        app:layout_constraintVertical_bias="0.127" />

    <ImageView
        android:id="@+id/breakFastIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.252"
        app:layout_constraintStart_toEndOf="@+id/wifiIcon"
        app:layout_constraintTop_toBottomOf="@+id/roomATxt"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/ic_bfast" />

    <TextView
        android:id="@+id/bfastTxt"
        android:layout_width="132dp"
        android:layout_height="22dp"
        android:fontFamily="@font/poppins_light"
        android:text="Free Breakfast"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toEndOf="@+id/breakFastIcon"
        app:layout_constraintTop_toTopOf="@+id/beachTxt"
        app:layout_constraintVertical_bias="0.127" />

    <ImageView
        android:id="@+id/dedicatedWorkspace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/wifiIcon"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_workspace" />

    <TextView
        android:id="@+id/workspaceTxt"
        android:layout_width="156dp"
        android:layout_height="20dp"
        android:fontFamily="@font/poppins_light"
        android:text="Dedicated Workspace"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toEndOf="@+id/dedicatedWorkspace"
        app:layout_constraintTop_toBottomOf="@+id/wifiTxt"
        app:layout_constraintVertical_bias="0.036" />

    <ImageView
        android:id="@+id/amenitiesIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/dedicatedWorkspace"
        app:layout_constraintVertical_bias="0.037"
        app:srcCompat="@drawable/ic_others" />

    <TextView
        android:id="@+id/amenitiesTxt"
        android:layout_width="201dp"
        android:layout_height="65dp"
        android:fontFamily="@font/poppins_light"
        android:text="Smoking Allowed, Carbon Monoxide Alarm, Smoke Alarm, Free Parking"
        android:textColor="@color/gray"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.313"
        app:layout_constraintStart_toEndOf="@+id/dedicatedWorkspace"
        app:layout_constraintTop_toBottomOf="@+id/workspaceTxt"
        app:layout_constraintVertical_bias="0.043" />

    <TextView
        android:id="@+id/ratingTxt"
        android:layout_width="278dp"
        android:layout_height="39dp"
        android:fontFamily="@font/poppins_light"
        android:text="• Rating | 5.0 | 7 Reviews"
        android:textColor="@color/gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/amenitiesTxt"
        app:layout_constraintVertical_bias="0.079" />

    <ImageView
        android:id="@+id/showAllReviewsRect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/ratingTxt"
        app:layout_constraintVertical_bias="0.304"
        app:srcCompat="@drawable/dark_green_rounded_rect" />

    <TextView
        android:id="@+id/showAllReviews"
        android:layout_width="144dp"
        android:layout_height="32dp"
        android:fontFamily="@font/poppins_light"
        android:text="Show All Reviews"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintStart_toStartOf="@+id/showAllReviewsRect"
        app:layout_constraintTop_toTopOf="@+id/ratingTxt"
        app:layout_constraintVertical_bias="0.36" />

    <Button
        android:id="@+id/okBtnCheckInTime"
        android:layout_width="63dp"
        android:layout_height="42dp"
        android:backgroundTint="@color/lemongreen"
        android:fontFamily="@font/poppins_medium"
        android:text="ok"
        android:textColor="@color/darkgreen"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/showAllReviews"
        app:layout_constraintVertical_bias="0.454" />

</androidx.constraintlayout.widget.ConstraintLayout>