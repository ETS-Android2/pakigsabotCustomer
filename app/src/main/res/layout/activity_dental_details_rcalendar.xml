<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:fillViewport="true"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/topbar_bg"
    tools:context=".DentalClinics.DentalDetailsRCalendar">

    <ImageView
        android:id="@+id/resortIcon"
        android:layout_width="53dp"
        android:layout_height="40dp"
        android:src="@drawable/cebu_wetland_icon_item"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imgBackBtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <ImageView
        android:id="@+id/imgBackBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="TODO"
        android:src="@drawable/ic_arrow_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/lblDental"
        android:layout_width="265dp"
        android:layout_height="37dp"
        android:fontFamily="@font/poppins_light"
        android:text="Smile Specialist Dental Center"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/resortIcon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/lblSerenityAddress"
        android:layout_width="173dp"
        android:layout_height="15dp"
        android:fontFamily="@font/poppins_light"
        android:text="Cebu IT Park, Cebu City"
        android:textColor="@color/lemongreen"
        android:textSize="10sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/lblDental"
        app:layout_constraintVertical_bias="0.028" />

    <ImageView
        android:id="@+id/cebuWetlandHeart"
        android:layout_width="36dp"
        android:layout_height="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toEndOf="@+id/resortIcon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/ic_heart" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="109dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.341"
        app:layout_constraintStart_toEndOf="@+id/noteTxt"
        app:layout_constraintTop_toBottomOf="@+id/lblSerenityAddress"
        app:layout_constraintVertical_bias="0.056"
        app:srcCompat="@drawable/cebu_wetland_map" />

    <TextView
        android:id="@+id/noteTxt"
        android:layout_width="165dp"
        android:layout_height="61dp"
        android:fontFamily="@font/poppins_light"
        android:text="Please make reservation at least 3 days prior to your preferred date."
        android:textColor="@color/black"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resortIcon"
        app:layout_constraintVertical_bias="0.075" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resortIcon"
        app:layout_constraintVertical_bias="0.061"
        app:srcCompat="@drawable/idea_icon" />

    <ImageView
        android:id="@+id/rcalendar_resort"
        android:layout_width="451dp"
        android:layout_height="261dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noteTxt"
        app:layout_constraintVertical_bias="0.065"
        app:srcCompat="@drawable/reservation_calendar" />

    <TextView
        android:id="@+id/notesTxt"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Note/s: "
        android:textColor="@color/darkgreen"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.282"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rcalendar_resort"
        app:layout_constraintVertical_bias="0.02" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="270dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notesTxt"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_white" />


    <TextView
        android:id="@+id/proceduresTxt"
        android:layout_width="85dp"
        android:layout_height="16dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Procedures"
        android:textColor="@color/darkgreen"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.309"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/promoTxt"
        android:layout_width="127dp"
        android:layout_height="19dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Promo and Deals"
        android:textColor="@color/darkgreen"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.355"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/proceduresTxt"
        app:layout_constraintVertical_bias="0.158" />

    <TextView
        android:id="@+id/cancellationTxt"
        android:layout_width="139dp"
        android:layout_height="17dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Cancellation Policy"
        android:textColor="@color/darkgreen"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.371"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/promoTxt"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt1"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="52dp"
        android:fontFamily="@font/poppins_medium"
        android:text="v"
        android:textColor="@color/darkbluegreen"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/proceduresTxt"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt2"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:fontFamily="@font/poppins_medium"
        android:text="v"
        android:textColor="@color/darkbluegreen"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toEndOf="@+id/cancellationTxt"
        app:layout_constraintTop_toBottomOf="@+id/txt3"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/txt3"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:layout_marginEnd="52dp"
        android:fontFamily="@font/poppins_medium"
        android:text="v"
        android:textColor="@color/darkbluegreen"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/promoTxt"
        app:layout_constraintTop_toBottomOf="@+id/txt1"
        app:layout_constraintVertical_bias="0.059" />

    <Button
        android:id="@+id/reserveBtnRDDC"
        android:layout_width="118dp"
        android:layout_height="56dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/darkgreen"
        android:fontFamily="@font/poppins_medium"
        android:text="RESERVE"
        android:textColor="@color/lemongreen"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cancellationTxt"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>